    "dist/",
    "loader/"
  ],
  "scripts": {
    "prod": "stencil build --hvProd --docs",
    "prod.watch": "stencil build --hvProd --watch --serve",
    "qa": "stencil build --hvQa",
    "qa.watch": "stencil build --hvQa --watch --serve",
    "dev": "stencil build --hvDev",
